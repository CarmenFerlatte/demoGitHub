{
    "name": "Weather",
    "orientation": "portrait",
    "description": "Une extension pour tester les manifestes",
    "manifest_version": 2,
    "version": "1.0",
    "protocol": "web+demo",
    "url": "/https://carmenferlatte.github.io/demoGitHub/",
    "short_name": "Weather",
    "icons": [{
        "src": "images/icons/icon-16x16.png",
        "sizes": "48x48",
        "type": "image/png"
    }, {
        "src": "images/icons/icon-48x48.png",
        "sizes": "72x72",
        "type": "image/png"
    }, {
        "src": "images/icons/icon-128x128.png",
        "sizes": "96x96",
        "type": "image/png"
    }, {
        "src": "images/icons/icon-152x152.png",
        "sizes": "128x128",
        "type": "image/png"
    }, {
        "src": "images/icons/icon-192x192.png",
        "sizes": "192x192",
        "type": "image/png"
    }, {
        "src": "images/icons/icon-256x256.png",
        "sizes": "384x384",
        "type": "image/png"
    }, {
        "src": "images/icons/icon-512x512.png",
        "sizes": "512x512",
        "type": "image/png"    
    }
],
"screenshots": [{
    "src": "screenshots/screenshot1.png",
    "sizes": "640x480",
    "type": "image/png"
}, {
    "src": "screenshots/screenshot2.png",
    "sizes": "1280x720",
    "type": "image/png"
}
],

"shortcuts": [
    {
        "name": "Accès rapide à la fonctionnalité 1",
        "description": "Description de la fonctionnalité 1",
        "url": "/https://carmenferlatte.github.io/demoGitHub/",
        "icons": [
            {
                "src": "images/shortcut-icon1.png",
                "sizes": "192x192"
            }
        ]
    },
    {
        "name": "Accès rapide à la fonctionnalité 2",
        "description": "Description de la fonctionnalité 2",
        "url": "/https://carmenferlatte.github.io/demoGitHub/",
        "icons": [
            {
                "src": "images/shortcut-icon2.png",
                "sizes": "192x192"
            }
        ]
    }

],

"protocol_handlers": [
    {
        "protocol": "web+demo",
        "name": "Ouvrir dans Weather",
        "url": "/https://carmenferlatte.github.io/demoGitHub/"
    }
],

"start_url": "/https://carmenferlatte.github.io/demoGitHub/",
"display": "standalone",
"display_override": ["fullscreen", "standalone", "minimal-ui", "browser"],
"background_color": "#f0f0f0",
"theme_color": "#f0f0f0"

}